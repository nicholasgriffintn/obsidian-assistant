{
        "name": "obsidian-assistant",
        "version": "0.0.17",
        "description": "A plugin for obsidian that links my notes to my assistant API",
        "main": "main.js",
        "type": "module",
        "scripts": {
                "dev": "vite build --mode development --watch",
                "build": "vite build --mode production",
                "check": "svelte-check --tsconfig ./tsconfig.json",
                "format": "biome format --write ./src",
                "lint": "biome lint --apply-unsafe ./src",
                "version": "npx tsx scripts/bump-version.ts"
        },
        "keywords": [],
        "author": "Nicholas Griffin",
        "license": "MIT",
        "devDependencies": {
                "@biomejs/biome": "1.3.3",
                "@sveltejs/vite-plugin-svelte": "^2.4.6",
                "@tsconfig/svelte": "^5.0.2",
                "@types/node": "^20.17.10",
                "@types/semver": "^7.5.8",
                "@unocss/extractor-svelte": "^0.57.2",
                "builtin-modules": "^3.3.0",
                "bun-types": "latest",
                "obsidian": "^1.4.11",
                "semver": "^7.6.3",
                "svelte": "^4.2.2",
                "svelte-check": "^3.5.2",
                "tslib": "^2.6.2",
                "typescript": "^5.2.2",
                "unocss": "^0.57.2",
                "vite": "^4.5.0"
        },
        "dependencies": {
                "@ai-sdk/openai-compatible": "^0.0.16",
                "@ai-sdk/provider": "^1.0.4",
                "@ai-sdk/provider-utils": "^2.0.7",
                "ai": "^4.0.33"
        }
}